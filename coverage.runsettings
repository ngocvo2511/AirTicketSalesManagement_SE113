<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
        <Configuration>
	  <Exclude>
            [AirTicketSalesManagement.View*]*
          </Exclude>
          <ExcludeByAttribute>
            ExcludeFromCodeCoverage
          </ExcludeByAttribute>
          <!-- Phòng hờ -->
          <ExcludeByFile>
            **/*.g.cs;
            **/obj/**;
            **/bin/**
          </ExcludeByFile>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>
